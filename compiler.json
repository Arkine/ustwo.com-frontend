{
  "name": "ustwo-com-frontend",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "compile": "gulp build",
    "compile-dev": "gulp build --dev",
    "start": "babel-node --optional es7.classProperties src/server",
    "dev": "gulp start --dev && npm run start",
    "styleguide": "gulp start --styleguide --dev && npm run start",
    "css": "gulp css --dev",
    "test": "mocha --compilers js:./src/app/lib/testdom.js,js:babel/register $(find test -name '*.js')"
  },
  "aliasify": {
    "aliases": {
      "../../adaptors/server/track": "./src/app/adaptors/client/track",
      "../adaptors/server/tween-max": "./src/app/adaptors/client/tween-max",
      "../../adaptors/server/tween-max": "./src/app/adaptors/client/tween-max",
      "../../adaptors/server/scroll-to-plugin": "./src/app/adaptors/client/scroll-to-plugin",
      "../../adaptors/server/localfont": "./src/app/adaptors/client/localfont",
      "../adaptors/server/tracking": "./src/app/adaptors/client/tracking",
      "../../adaptors/server/tracking": "./src/app/adaptors/client/tracking",
      "../adaptors/server/env": "./src/app/adaptors/client/env",
      "../../adaptors/server/env": "./src/app/adaptors/client/env",
      "isomorphic-fetch/fetch-npm-node.js": "isomorphic-fetch/fetch-npm-browserify.js",
      "../adaptors/server/window": "./src/app/adaptors/client/window",
      "../../adaptors/server/scroll-magic": "./src/app/adaptors/client/scroll-magic",
      "../adaptors/server/proxy-url": "./src/app/adaptors/client/proxy-url",
      "../../adaptors/server/svg4everybody": "./src/app/adaptors/client/svg4everybody",
      "../adaptors/server/scroll-tracker": "./src/app/adaptors/client/scroll-tracker"
    }
  },
  "babel": {
    "optional": ["es7.classProperties"]
  },
  "dependencies": {
    "aliasify": "1.7.2",
    "autoprefixer": "6.0.3",
    "babel": "5.8.21",
    "babelify": "6.1.3",
    "browserify": "11.0.0",
    "browsernizr": "1.0.2",
    "classnames": "2.1.3",
    "chai": "3.2.0",
    "consolidate": "0.13.1",
    "del": "1.2.0",
    "ellipsize": "0.0.2",
    "es6-promise": "2.3.0",
    "express": "4.13.1",
    "gsap": "1.17.0",
    "gulp": "3.9.0",
    "gulp-if": "1.2.5",
    "gulp-mocha": "2.1.3",
    "gulp-postcss": "6.0.0",
    "gulp-sass": "2.0.4",
    "gulp-sourcemaps": "1.5.2",
    "gulp-uglify": "1.2.0",
    "gulp-util": "3.0.6",
    "he": "0.5.0",
    "isomorphic-fetch": "2.1.1",
    "jsdom": "3.1.0",
    "lodash": "3.10.0",
    "mocha": "2.3.2",
    "moment": "2.10.6",
    "normalize-libsass": "1.0.3",
    "pluralize": "1.1.2",
    "react": "0.13.3",
    "react-helmet": "1.1.0",
    "react-style-guide": "1.1.5",
    "react-transition-manager": "1.2.3",
    "route-pattern": "0.0.6",
    "scrollmagic": "2.0.5",
    "serve-favicon": "2.3.0",
    "sinon": "1.16.1",
    "sinon-chai": "2.8.0",
    "susy": "2.2.5",
    "svg4everybody": "1.0.0",
    "urllite": "0.5.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "yargs": "3.15.0"
  }
}
