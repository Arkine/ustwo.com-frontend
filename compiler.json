{
  "name": "ustwo-com-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile": "gulp build",
    "compile-dev": "gulp build --dev",
    "start": "babel-node --optional es7.classProperties src/server",
    "dev": "gulp start --dev && npm run start",
    "styleguide": "gulp start --styleguide --dev && npm run start",
    "css": "gulp css --dev"
  },
  "aliasify": {
    "aliases": {
      "../../server/adaptors/track": "./src/client/adaptors/track",
      "../server/adaptors/tween-max": "./src/client/adaptors/tween-max",
      "../../server/adaptors/tween-max": "./src/client/adaptors/tween-max",
      "../server/adaptors/scroll-to-plugin": "./src/client/adaptors/scroll-to-plugin",
      "../server/adaptors/localfont": "./src/client/adaptors/localfont",
      "../server/adaptors/tracking": "./src/client/adaptors/tracking",
      "../../server/adaptors/tracking": "./src/client/adaptors/tracking",
      "../server/adaptors/env": "./src/client/adaptors/env",
      "../../server/adaptors/env": "./src/client/adaptors/env",
      "isomorphic-fetch/fetch-npm-node.js": "isomorphic-fetch/fetch-npm-browserify.js",
      "../../server/adaptors/window": "./src/client/adaptors/window",
      "../../server/adaptors/scroll-magic": "./src/client/adaptors/scroll-magic",
      "../../server/adaptors/proxy-url": "./src/client/adaptors/proxy-url",
      "../server/adaptors/svg4everybody": "./src/client/adaptors/svg4everybody"
    }
  },
  "dependencies": {
    "aliasify": "1.7.2",
    "autoprefixer-core": "5.2.1",
    "babel": "5.8.21",
    "babelify": "6.1.3",
    "browserify": "11.0.0",
    "browsernizr": "1.0.2",
    "classnames": "2.1.3",
    "consolidate": "0.13.1",
    "del": "1.2.0",
    "es6-promise": "2.3.0",
    "express": "4.13.1",
    "gsap": "1.17.0",
    "gulp": "3.9.0",
    "gulp-if": "1.2.5",
    "gulp-mocha": "2.1.3",
    "gulp-postcss": "5.1.10",
    "gulp-sass": "2.0.4",
    "gulp-sourcemaps": "1.5.2",
    "gulp-uglify": "1.2.0",
    "gulp-util": "3.0.6",
    "he": "0.5.0",
    "isomorphic-fetch": "2.1.1",
    "lodash": "3.10.0",
    "moment": "2.10.6",
    "normalize-libsass": "1.0.3",
    "pluralize": "1.1.2",
    "react": "0.13.3",
    "react-helmet": "1.1.0",
    "react-style-guide": "1.1.5",
    "react-transition-manager": "1.2.3",
    "route-pattern": "0.0.6",
    "scrollmagic": "2.0.5",
    "serve-favicon": "2.3.0",
    "susy": "2.2.5",
    "svg4everybody": "1.0.0",
    "urllite": "0.5.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "yargs": "3.15.0"
  }
